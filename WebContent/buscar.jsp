<body>
	
	<div class="limiter">
		<div class="container-login100" style="background-image: url('images/bg-01.jpg');">
			<div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
				<form class="login100-form validate-form">
					<span class="login100-form-title p-b-49">
						Buscar
					</span>
<a href="/nomina">Regresar</a>
					<div class="wrap-input100 validate-input m-b-23" data-validate = "Username is reauired">
						<span class="label-input100">Codigo Empleado</span>
						<input class="input100" type="text" name="codigo" id="codigo" placeholder="Type your username">
						<span class="focus-input100" data-symbol="&#xf206;"></span>
					</div>
					
<br>
					
					<div class="container-login100-form-btn">
						<div class="wrap-login100-form-btn">
							<div class="login100-form-bgbtn"></div>
							<a class="login100-form-btn" id="ir-buscar">
								Buscar
							</a>
						</div>
					</div>
				</form>
				<div id="datos">
				<c:if test="${empty emp}">
    	Usuario No Existe
</c:if>

<c:if test="${not empty emp}">
    	<p>Código: ${emp.codigo}</p>
    	<p>Nombre: ${emp.nombre}</p>
<p>Cédula: ${emp.cedula}</p>

<p>Fecha De Nacimiento: ${emp.fechanacimiento}</p>
<p>Fecha Ingreso: ${emp.fechaingreso}</p>
<p>Fecha Retiro: ${emp.fecharetiro}</p>
</c:if>
